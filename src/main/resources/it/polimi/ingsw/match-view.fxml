   <children>
      <TabPane fx:id="tabPane">
          <Tab closable="false" text="Table">
              <fx:include fx:id="table" source="table-view.fxml" />
          </Tab>
          <Tab closable="false" text="Nickname1's School">
              <fx:include fx:id="school1" source="school-view.fxml" />
          </Tab>
          <Tab closable="false" text="Nickname2's School">
              <fx:include fx:id="school2" source="school-view.fxml" />
          </Tab>
          <Tab closable="false" text="Nickname3's School">
              <fx:include fx:id="school3" source="school-view.fxml" />
          </Tab>
          <Tab closable="false" text="Nickname4' School">
              <fx:include fx:id="school4" source="school-view.fxml" />
          </Tab>
      </TabPane>
      <Button layoutX="842.0" layoutY="2.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>
